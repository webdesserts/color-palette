{
  "name": "color-palette",
  "version": "1.0.0",
  "description": "spike to see if we like react",
  "main": "index.js",
  "author": "Michael C. Mullins",
  "license": "ISC",
  "scripts": {
    "start": "node devServer.js",
    "build:sass": "node-sass --source-map=true app/styles/main.scss -o app/assets",
    "watch:sass": "node-sass -w --source-map=true app/styles/main.scss -o app/assets",
    "build": "npm run build:sass && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "NODE_ENV=test zuul --local --open -- app/**/__tests__/**/*.{js,jsx}",
    "lint": "eslint app/{*,**/*}.{js,jsx}",
    "lint-fix": "eslint app/{*,**/*}.{js,jsx} --fix"
  },
  "dependencies": {
    "history": "^1.13.0",
    "immutable": "~3.7",
    "isomorphic-fetch": "~2.2",
    "react": "~0.14",
    "react-dom": "~0.14",
    "react-redux": "~4.0",
    "react-router": "~1",
    "redux": "~3.0.4",
    "redux-simple-router": "0.0.7",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "~5",
    "babel-eslint": "^4",
    "babel-loader": "~5",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "~3",
    "eslint": "~1",
    "eslint-plugin-babel": "~2",
    "eslint-plugin-react": "~3",
    "express": "~4",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "~0.14",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.1.1",
    "redux-devtools": "~2",
    "redux-devtools-diff-monitor": "~3",
    "rimraf": "^2.4.3",
    "webpack": "~1",
    "webpack-dev-middleware": "~1",
    "webpack-hot-middleware": "~2",
    "zuul": "~3",
    "zuul-builder-webpack": "^1.1.0"
  }
}
